interface Par1{
	void hello();
}

interface Par2{
	void hello();
}

class Chi implements Par1, Par2{
	public void hello() {
		System.out.println("Hello");
	}
}

class Parent{
	int parVar;
	static int thisIsStatic = 10;
	public Parent(int a, int b) {
		parVar = a;
		System.out.println("Called in parent");
	}
	void show() {
		System.out.println("parentVariable = " + parVar);
		System.out.println("Inside the parent show method");
	}
	
}

class Child extends Parent{
	public Child(int c, int b, int a) {
		super(a, b);
		System.out.println("Called in child");
	}
	void show() {
		super.show();
		System.out.println("parentVariable = " + parVar);
		System.out.println("Inside the child show method");
	}
}

class Counter{
	static int count = 0;
	static void inc() {
		count++;
	}
}

